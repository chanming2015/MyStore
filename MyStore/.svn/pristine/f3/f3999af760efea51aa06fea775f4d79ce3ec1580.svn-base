		return orderUser;
	}
	public void setOrderUser(OrderUser orderUser) {
		this.orderUser = orderUser;
	}
	/**
	 * @return the iNum
	 */
	public int getiNum() {
		return iNum;
	}
	/**
	 * @param iNum the iNum to set
	 */
	public void setiNum(int iNum) {
		this.iNum = iNum;
	}
	/**
	 * 
