/* CSS Document */
.whole {
	width: 1200px;
	height: 100%;
	margin: auto;
}

iframe {
	border: 0px;
}

.topframe {
	width: 1200px;
	height: 170px;
}

.mainframe {
	width: 1200px;
}

.bottomframe {
	padding-bottom: 1px;
	width: 1200px;
	height: 95px;
}

ul {
	list-style-type: none;
}
/*头部顶部大DIV*/
.top {
	position: fixed;
	top: 0;
	width: 100%;
	height: 180px;
	border-bottom: 1px solid #eee;
	z-index: 1000;
}

.top-top {
	margin: 0 auto;
	top: 0;
	width: 100%;
	height: 35px;
	border-bottom: 1px solid #eee;
	background-color: #f5f5f5;
}
/*头部底部顶部DIV主体*/
.top-info {
	width: 1190px;
	height: 100%;
	margin: auto;
}
/*底部顶部大DIV*/
.bottom {
	bottom: 0px;
	width: 100%;
	height: 110px;
	border-bottom: 1px solid #eee;
	padding-bottom: 1px;
	clear: both;
}

.bottom-info {
	width: 1190px;
	height: 100%;
	margin: auto;
}
/*头部顶部DIV左边部分*/
.top-left {
	position: relative;
	float: left;
	width: 30%;
	height: 100%;
	line-height: 35px;
}
/*头部顶部DIV左边部分*/
.top-right {
	position: relative;
	float: right;
	height: 100%;
	width: 230px;
	line-height: 35px;
}
/*所有A标签*/
.biaoqian {
	margin: 0;
	padding: 0;
	text-decoration: none; /*无下划线*/
	font-size: 100%;
	vertical-align: baseline; /*默认对齐方式*/
	background: transparent; /*透明背景*/
	color: #6c6c6c;
}
/*A标签鼠标放上后*/
.biaoqian:hover {
	color: #F40;
	transition: color .2s ease-in-out; /*缓慢变色*/
}
/*头部DIV主体*/
.top-main {
	margin: 0 auto;
	width: 1190px;
	height: 145px;
}
/*头部DIV主体图片左漂*/
.top-main>img {
	float: left;
}
/*搜索框主体*/
.search {
	float: left;
	margin-top: 50px;
	width: 730px;
	height: 75%;
}
/*搜索框主体盒子*/
.search-box {
	position: relative;
	float: left;
	margin-right: 10px;
	width: 674px;
	height: 40px;
	border: 3px solid #F40;
}
/*搜索框主体盒子前段部分*/
.baobei {
	float: left;
	width: 72px;
	height: 40px;
	border-right: 1px solid #e5e5e5;
	border-left: 1px solid #fff;
	line-height: 40px;
	text-align: center;
	background: #f6f6f6;
}

.userback {
	background-image: url(/MyStore/img/user/user-bg.png);
	background-repeat: no-repeat;
	background-position: right top;
	height: 178px;
	width: 100%;
}

.login-btn {
	display: inline-block;
	width: 100px;
	height: 35px;
	text-align: center;
	line-height: 35px;
	letter-spacing: 1px;
	border-bottom: 1px solid #e4e4e4;
	background: #f40;
	color: #fff;
	text-decoration: none;
}

.order-btn {
	cursor: pointer;
	width: 90px;
	height: 35px;
	text-align: center;
	line-height: 35px;
	letter-spacing: 1px;
	border: 1px solid #f40;
	background: #f40;
	color: #fff;
	text-decoration: none;
}

.login-btn:hover {
	background-color: #E83E00;
	color: #fff;
}

.footer-nav {
	width: 100%;
	height: 42px;
	line-height: 42px;
	border-bottom: 1px solid #ddd;
	color: #dedede;
}

.footer-nav>a {
	margin: 0;
	padding: 0;
	text-decoration: none; /*无下划线*/
	font-size: 100%;
	vertical-align: baseline; /*默认对齐方式*/
	background: transparent; /*透明背景*/
	color: #6c6c6c;
}
/*搜索框主体盒子主体部分*/
.search-in {
	float: left;
	width: 495px;
	height: 38px;
	padding-left: 10px;
	border: none;
	outline: none;
	background: url(../img/search.png) no-repeat 98% center,
		url(../img/s.png) no-repeat 2% center;
}
/*搜索按钮*/
.search-but {
	float: right;
	width: 95px;
	height: 40px;
	border: none;
	outline: none;
	font-size: 20px;
	letter-spacing: 5px;
	background: #F40;
	cursor: pointer;
	color: #fff;
}

.search-but:hover {
	background-color: red;
}
/*main主体*/
.content {
	width: 1190px;
	/* min-height: 700px; */
	height: 371px;
	margin: 185px auto;
	clear: both;
	z-index: -1;
	/*border: 1px solid #ccc;*/
}
/*main侧边栏*/
.sidebar {
	float: left;
	width: 150px;
	height: 373px;
	box-shadow: 2px 2px 2px #e1e1e1; /*添加阴影*/
}
/*main侧边栏内容*/
.sidebar-info {
	position: relative;
	width: 148px;
	height: 331px;
	border: 1px solid #bbb;
	border-top: none;
}
/*main侧边栏内容h3标签*/
.sidebar-info h3 {
	width: 93%;
	height: 100%;
	line-height: 31px;
	margin: 0 auto;
	padding: 0;
	text-align: center;
	border-bottom: 1px solid #dedede;
	color: #707070;
}
/*main侧边栏h3标签*/
.sidebar>h3 {
	margin: 0;
	width: 100%;
	height: 41px;
	line-height: 41px;
	text-align: center;
	font-size: 14px;
	background: #F40;
	color: #fff;
}

/*main侧边栏列表*/
.side-li {
	list-style-type: none;
	position: relative;
	text-align: center;
}
/*main侧边栏列表下的LI标签*/
.side-li>li {
	width: 147px;
	margin: auto;
	height: 31px;
	line-height: 32px;
	text-align: center;
	border: 1px solid #fff;
	border-right: none;
	cursor: pointer; /*指针变手*/
}

/*侧边栏点开盒子模型*/
.hiden-box {
	position: fixed;
	width: 709px;
	height: 530px;
	padding: 0px;
	display: none;
	border: 1px solid #F40;
	overflow: hidden;
	background: #FFFFFF;
}
/*侧边栏点开盒子列表*/
.hiden-box>li {
	width: 96%;
	margin: auto;
	height: 100%;
}
/*侧边栏点开盒子*/
.right-box {
	margin: 0 40px 0 5px;
	padding: 1px 6px;
	border: 1px solid #eee;
	cursor: pointer;
}
/*上面的大标题*/
.hidden-title {
	width: 100%;
	height: 45px;
	line-height: 45px;
	border-bottom: 1px solid #eee;
}
/*大标题下的小标题*/
.sub-nav-right {
	float: left;
	padding-right: 10px;
	width: 340px;
	height: 420px;
	border-right: 1px solid #eee;
}

.sub-nav-left {
	float: right;
	padding-left: 10px;
	width: 330px;
	height: 420px;
}
/*侧边栏点开盒子展示*/
.cell-box {
	width: 100%;
	height: 116px;
	margin-top: 16px;
	border-bottom: 1px dashed #eee;
}
/*侧边栏点开盒子展示h1*/
.cell-box>h1 {
	margin-bottom: 10px;
}
/*侧边栏展开盒子下的小模块*/
.a-box {
	line-height: 20px;
	color: #707070;
}

.a-box span {
	margin-right: 6px;
}

.a-box>a {
	margin: 0 8px 0 0;
	white-space: nowrap;
	color: #707070;
}

.a-box>a:hover {
	color: #F40;
}

/*main侧边栏右边*/
.right-con {
	width: 1031px;
	height: 373px;
	float: right;
}
/*main侧边栏右边顶部*/
.nav {
	width: 100%;
	height: 41px;
	line-height: 38px;
	font-weight: bold;
	font-size: 14px;
	border-bottom: 2px solid #F40;
}
/*图片轮播展示盒子*/
.show-box {
	float: left;
	width: 720px;
	height: 500px;
}
/*图片轮播展示容器*/
.content-img,.content-down,.content-sub {
	position: relative;
	float: left;
	width: 520px;
	height: 280px;
	overflow: hidden;
}
/*图片轮播相关*/
.imgBox,.imgBox1,.imgBox2 {
	position: absolute;
	clear: both;
	width: 2600px;
	list-style-type: none;
}

.imgBox2 {
	width: 660px;
}

.imgBox>li,.imgBox1>li {
	float: left;
	width: 520px;
}

.imgBox2>li {
	float: left;
	width: 220px;
}

.imgBox a,.imgBox1 a,.imgBox2 a {
	display: inline-block;
}
/*%宽*/
.width {
	width: 100%;
}
/*%高*/
.height {
	height: 100%;
}
/*%宽 高*/
.baifen {
