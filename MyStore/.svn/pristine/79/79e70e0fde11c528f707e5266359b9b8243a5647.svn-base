/*goodshowdonghua*/
.box {
	width: 1190px;
	margin: 0 auto;
	/* padding-top: 50px; */
	height: 300px;
	min-height: 650px;
}

.border_animation {
	border: 2px solid #eee;
	width: 220px;
	height: 280px;
	float: left;
	margin-left: 5px;
	margin-right: 5px;
	position: relative;
}

.border_animation .border_top {
	position: absolute;
	height: 2px;
	width: 0;
	font-size: 0;
	background: #f40;
	top: 0;
	left: 0;
	transition: all 0.5s ease-out;
}

.border_animation .border_right {
	position: absolute;
	height: 0px;
	width: 2px;
	font-size: 0;
	background: #f40;
	bottom: 0;
	right: 0;
	transition: all 0.5s ease-out;
}

.border_animation .border_bottom {
	position: absolute;
	height: 2px;
	width: 0px;
	font-size: 0;
	background: #f40;
	right: 0;
	bottom: 0;
	transition: all 0.5s ease-out;
}

.border_animation .border_left {
	position: absolute;
	height: 0px;
	width: 2px;
	font-size: 0;
	background: #f40;
	left: 0;
	top: 0;
	transition: all 0.5s ease-out;
}

.eva-switchable-panels img {
	display: block;
}

.box .hover .border_top,.box .hover .border_bottom {
	width: 220px;
}

.box .hover .border_left,.box .hover .border_right {
	height: 280px;
}

