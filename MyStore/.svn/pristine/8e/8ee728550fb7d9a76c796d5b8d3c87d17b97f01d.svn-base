public class GoodsIamgeList {
	
	private Goods goods;
	private List<ImageGoods> imageList;
	
	public Goods getGoods() {
		return goods;
	}
	public void setGoods(Goods goods) {
		this.goods = goods;
	}
	public List<ImageGoods> getImageList() {
		return imageList;
	}
	public void setImageList(List<ImageGoods> imageList) {
		this.imageList = imageList;
	}
	public GoodsIamgeList(Goods goods, List<ImageGoods> imageList) {
		this.goods = goods;
		this.imageList = imageList;
	}
}
